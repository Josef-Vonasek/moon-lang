{
  "name": "moon-lang",
  "version": "0.1.35",
  "description": "Universal code-interchange format",
  "main": "lib/moon-lang.js",
  "scripts": {
    "babel": "babel src --out-dir=lib",
    "browserify": "browserify lib/moon-lang.js > dist/moon-lang.bundle.js",
    "minify": "ccjs dist/moon-lang.bundle.js > dist/moon-lang.min.js",
    "build": "npm run babel; npm run browserify; npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maiavictor/moon.git"
  },
  "author": "Victor Maia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maiavictor/moon/issues"
  },
  "homepage": "https://github.com/maiavictor/moon#readme",
  "dependencies": {
    "xhr-request-promise": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "browserify": "^14.4.0",
    "ccjs": "^0.2.0"
  }
}
